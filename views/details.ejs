                                <li><i class="fa fa-star"></i><span class="box_1" style="color: white;"></span></li>
                                <li><i class="fa fa-users"></i><span class="box_2" style="color: white;"></span></li>
                                <li><i class="fa fa-chalkboard-teacher"></i><span class="box_3"
                                    style="color: white;"></span>
                                </li>
                                <li><i class="fa fa-dollar-sign"></i><span class="box_4" style="color: white;"></span>
                                </li>
                              </ul>
                            </div>
                          </div>
                          <div class="row" id="extra_img">
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- ***** Details End ***** -->

                <!-- ***** Other Start ***** -->
                <div class="other-games">
                  <div class="row">
                    <div class="col-lg-12">
                      <div class="heading-section">
                        <h4><em>Other Related</em> Gigs</h4>
                      </div>
                    </div>
                    <div id="reconmondGigs" class="row">

                    </div>

                  </div>
                </div>
              </div>
              <!-- ***** Other End ***** -->

            </div>
  </div>
  </div>
