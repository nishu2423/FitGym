<body>

  <!-- ***** Preloader Start ***** -->
  <div id="js-preloader" class="js-preloader">
    <div class="preloader-inner">
      <span class="dot"></span>
      <div class="dots">
        <span></span>
        <span></span>
        <span></span>
      </div>
    </div>
  </div>
  <!-- ***** Preloader End ***** -->

  <!-- ***** Header Area Start ***** -->
  <header class="header-area header-sticky">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <nav class="main-nav">
            <!-- ***** Logo Start ***** -->
            <a href="/index" class="logo">
              <img src=" images/rlogob.png" style="position: absolute;width: 120px;" alt="">
            </a>
            <!-- ***** Logo End ***** -->
            <!-- ***** Search End ***** -->
            <div class="search-input">
              <form id="search" action="#">
                <input type="text" placeholder="Type Something" id='searchText' name="searchKeyword"
                  onkeypress="handle" />
                <i class="fa fa-search"></i>
              </form>
            </div>
            <!-- ***** Search End ***** -->
            <!-- ***** Menu Start ***** -->
            <ul class="nav">
              <li><a href="/" class="active">Home</a></li>
              <li><a href="/course">Course</a></li>
              <li><a href="/profile">Profile <img src='<%=userprofile%>' alt=""></a></li>
            </ul>
            <a class='menu-trigger'>
              <span>Menu</span>
            </a>
            <!-- ***** Menu End ***** -->
          </nav>
        </div>
      </div>
    </div>
  </header>
  <!-- ***** Header Area End ***** -->

  <div class="container">
    <% if( error !="" ){ %>
      <div class="error det" role="alert" >
        <strong> Error </strong>
        <%= error %>
          <button type="button" onclick="document.getElementsByClassName('error')[0].remove()"
            class="btn-close">x</button>
      </div>
      <% } %>
        <% if( error_messages !="" ){ %>
          <div class="error" role="alert">
            <strong> Error </strong>
            <%= error_messages %>
              <button type="button" onclick="document.getElementsByClassName('error')[0].remove()"
                class="btn-close">x</button>
          </div>
          <% } %>
            <div class="row">
              <div class="col-lg-12">
                <div class="page-content">

                  <!-- ***** Featured Start ***** -->
                  <div class="row">
                    <div class="col-lg-12">
                      <div class="feature-banner header-text">
                        <div class="row">
                          <div class="col-lg-4" id="feature-left">

                          </div>
                          <div class="col-lg-8">
                            <div class="thumb">
                              <p id="desc" style="color: white;"></p>
                            </div>
                          </div>
                          <div class="extradeatil">
                            <div class="shoping" id="shop_btn">

                            </div>

                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!-- ***** Featured End ***** -->

                  <!-- ***** Details Start ***** -->
                  <div class="game-details">
                    <div class="row">
                      <div class="col-lg-12">
                        <h2><span id="gigName"></span> Details</h2>
                      </div>
                      <div class="col-lg-12">
                        <div class="content">
                          <div class="row" <div class="col-lg-6">
                            <div class="right-info">
